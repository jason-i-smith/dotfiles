# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

# User specific environment and startup programs

GOPATH=$HOME/go
PATH=$PATH:$HOME/.local/bin:$HOME/bin:/snap/bin:$HOME/go/bin
MINISHIFT_HOME="/mnt/vm-images"
EDITOR=vim

export GOPATH
export PATH
export MINISHIFT_HOME
export EDITOR
